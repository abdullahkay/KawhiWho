{
    "manifest_version": 2,
    "name": "KawhiWho",
    "version": "1.0",
    "description": "It replaces the occurences of 'kawhi', 'kawhi leonard' to 'number 2', 'number 2'",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "replace.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}
